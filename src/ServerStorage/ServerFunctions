local func = {
    event ={

    },
    condition = {
        
    },
    Definations = {
        ["nbt"] = "The value is part of the nbt of a entity",
        ["behavior"] = "Defines which function00",
    },
    conditionSigns = {
        ["="] = function(c1,c2)
            local is = false
            if c1 == c2 then
                is = true
            end
            return is
        end,
        [">"] = function(c1,c2)
            local is = false
            if c1 > c2 then
                is = true
            end
            return is
        end,
        ["<"] = function(c1,c2)
            local is = false
            if c1 < c2 then
                is = true
            end
            return is
        end,
        ["<="] = function(c1,c2)
            local is = false
            if c1 <= c2 then
                is = true
            end
            return is
        end,
        [">="] = function(c1,c2)
            local is = false
            if c1 >= c2 then
                is = true
            end
            return is
        end,
        ["found"] = function(c1)
            local is = false
            if c1 then
                is = true
            end
            return is
        end,
        ["not"] = function(c1)
            local is = false
            if not c1 then
                is = true
            end
            return is
        end,
    },
}
